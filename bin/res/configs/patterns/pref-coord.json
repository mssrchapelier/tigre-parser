[
    {
        "comment": "coord (and)",
        "regex": "^(?<coord>wa)(?<stem>.+)$",
        "replace": "${coord}:COORD-\\[${stem}\\]:#"
    },
    {
        "comment": "coord (and, so, therefore, then)",
        "regex": "^(?<coord>ka)(?<stem>.+)$",
        "replace": "${coord}:COORD-\\[${stem}\\]:#"
    },
    {
        "comment": "subord (that)",
        "regex": "^(?<coord>km)(?<stem>.+)$",
        "replace": "${coord}:SUBORD-\\[${stem}\\]:#"
    }
]